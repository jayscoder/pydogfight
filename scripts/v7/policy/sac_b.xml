<Root name="V4RL">
    <V7Init/>
    <include path="v7/policy/reward.xml"/>

    <ReactiveSelector>
        <include path="v7/policy/handle_abnormal.xml"/>

        <V7FireAndGoToLocation1V1
                name="SACRLFireAndGoToLocation1V1"
                algo="SAC"
                reward_scope="default,attack,control"
                path="{{models_dir}}/SACRLFireAndGoToLocation1V1"
                log_interval="1"
                verbose="0"
                train="{{train}}"
        />
    </ReactiveSelector>
</Root>
